CREATE TABLE `jaap` (
	`id` INT NULL,
	`latitude` DOUBLE NULL DEFAULT NULL,
	`longitude` DOUBLE NULL DEFAULT NULL,
	`country` VARCHAR(50) NULL DEFAULT NULL,
	`city` VARCHAR(50) NULL DEFAULT NULL,
	`description` VARCHAR(50) NULL DEFAULT NULL,
	`updated` DATE NULL DEFAULT NULL
)
COLLATE='utf8mb4_general_ci'
;
SELECT `DEFAULT_COLLATION_NAME` FROM `information_schema`.`SCHEMATA` WHERE `SCHEMA_NAME`='jaap';
SHOW TABLE STATUS FROM `jaap`;
SHOW FUNCTION STATUS WHERE `Db`='jaap';
SHOW PROCEDURE STATUS WHERE `Db`='jaap';
SHOW TRIGGERS FROM `jaap`;
SELECT *, EVENT_SCHEMA AS `Db`, EVENT_NAME AS `Name` FROM information_schema.`EVENTS` WHERE `EVENT_SCHEMA`='jaap';
SELECT * FROM `information_schema`.`COLUMNS` WHERE TABLE_SCHEMA='jaap' AND TABLE_NAME='jaap' ORDER BY ORDINAL_POSITION;
SHOW INDEXES FROM `jaap` FROM `jaap`;
SELECT * FROM information_schema.REFERENTIAL_CONSTRAINTS WHERE   CONSTRAINT_SCHEMA='jaap'   AND TABLE_NAME='jaap'   AND REFERENCED_TABLE_NAME IS NOT NULL;
SELECT * FROM information_schema.KEY_COLUMN_USAGE WHERE   TABLE_SCHEMA='jaap'   AND TABLE_NAME='jaap'   AND REFERENCED_TABLE_NAME IS NOT NULL;
SHOW CREATE TABLE `jaap`.`jaap`;
SELECT CONSTRAINT_NAME, CHECK_CLAUSE FROM `information_schema`.`CHECK_CONSTRAINTS` WHERE CONSTRAINT_SCHEMA='jaap' AND TABLE_NAME='jaap';